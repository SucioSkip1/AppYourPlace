<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/des_btn"
    tools:context=".cascaron_negocio_interfazNegocio">

    <ImageView
        android:id="@+id/img_cascaron_negocio"
        android:layout_width="304dp"
        android:layout_height="244dp"
        android:background="@drawable/diseno_txt"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059"
        tools:ignore="ImageContrastCheck"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/btn_gps_cascaron_negocio"
        style="@style/diseño_botones"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:onClick="irACatalogo"
        android:text="Ver catalogo"
        android:textColor="#000000"
        android:textSize="13dp"
        android:textStyle="bold|italic"
        app:backgroundTint="@null"
        app:icon="@drawable/catalogo"
        app:iconSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.745"
        tools:ignore="TextSizeCheck" />

    <Button
        android:id="@+id/btn_ver_citas_cascaron_interfaz_negocio"
        style="@style/diseño_botones"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:onClick="irVerCitas"
        android:text="Ver citas"
        android:textColor="#000000"
        android:textSize="13dp"
        android:textStyle="bold|italic"
        app:backgroundTint="@null"
        app:icon="@drawable/opinion"
        app:iconSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.842"
        tools:ignore="TextSizeCheck" />

    <Button
        android:id="@+id/btn_ver_chats_cascaron"
        style="@style/diseño_botones"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:text="Ver chats"
        android:textColor="#000000"
        android:textSize="13dp"
        android:textStyle="bold|italic"
        app:backgroundTint="@null"
        app:icon="@drawable/ema"
        app:iconSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.859"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.842"
        tools:ignore="TextSizeCheck" />

    <Button
        android:id="@+id/btn_publicar_anuncio_cascaron_interfaz_negocio"
        style="@style/diseño_botones"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:onClick="irPublicarAnuncio"
        android:text="Publicar anuncio"
        android:textColor="#000000"
        android:textSize="13dp"
        android:textStyle="bold|italic"
        app:backgroundTint="@null"
        app:icon="@drawable/publi"
        app:iconSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.859"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.745"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/txt_des_cascaron_negocio_interfaz"
        android:layout_width="337dp"
        android:layout_height="124dp"
        android:background="@drawable/dis_back_txt"
        android:gravity="center"
        android:text="Prueba"
        android:textColor="#000000"
        android:textSize="25dp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.583" />

    <TextView
        android:id="@+id/txt_nombre_cascaron_negocio_interfaz"
        style="@style/diseño_botones"
        android:layout_width="335dp"
        android:layout_height="41dp"
        android:layout_marginStart="38dp"
        android:layout_marginEnd="38dp"
        android:background="@drawable/dis_txt"
        android:gravity="center"
        android:text="Prueba"
        android:textColor="#000000"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.424" />

    <Button
        android:id="@+id/btn_programar_local_cascaron"
        style="@style/diseño_botones"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:text="Programar local"
        android:textColor="#000000"
        android:textSize="13dp"
        app:icon="@drawable/not"
        app:iconSize="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.948"
        tools:ignore="TextSizeCheck" />

    <Button
        android:id="@+id/bt_ver_opiniones_cascaron_negocio"
        style="@style/diseño_botones"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:onClick="irVerOpiniones"
        android:text="Ver opiniones"
        android:textColor="#212121"
        android:textSize="13dp"
        app:icon="@android:drawable/stat_notify_chat"
        app:iconSize="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.859"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.948"
        tools:ignore="TextSizeCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>